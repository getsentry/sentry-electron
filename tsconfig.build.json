{
  "extends": "./node_modules/@sentry-internal/typescript/tsconfig.json",
  "include": ["src/**/*.ts"],
  "exclude": ["node_modules", "src/preload/**/*.ts"],
  "compilerOptions": {
    "skipLibCheck": true,
    "baseUrl": ".",
    "lib": ["ES7", "DOM"],
    "module": "ESNext",
    "outDir": ".",
    "rootDir": "src",
    "target": "ES6",
    "esModuleInterop": true
  }
}
