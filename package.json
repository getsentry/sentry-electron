{
  "name": "@sentry/electron",
  "description": "Official Sentry SDK for Electron",
  "version": "7.8.0",
  "main": "./index.js",
  "module": "./esm/main/index.js",
  "browser": "./esm/renderer/index.js",
  "exports": {
    ".": {
      "node": {
        "require": "./index.js",
        "import": "./esm/main/index.js"
      },
      "default": "./esm/renderer/index.js"
    },
    "./main": {
      "require": {
        "types": "./main/index.d.ts",
        "default": "./main/index.js"
      },
      "import": {
        "types": "./esm/main/index.d.ts",
        "default": "./esm/main/index.js"
      }
    },
    "./renderer": {
      "require": {
        "types": "./renderer/index.d.ts",
        "default": "./renderer/index.js"
      },
      "import": {
        "types": "./esm/renderer/index.d.ts",
        "default": "./esm/renderer/index.js"
      }
    },
    "./preload": {
      "require": {
        "types": "./preload/default.d.ts",
        "default": "./preload/default.js"
      },
      "import": {
        "types": "./esm/preload/default.d.ts",
        "default": "./esm/preload/default.js"
      }
    },
    "./preload-namespaced": {
      "require": {
        "types": "./preload/index.d.ts",
        "default": "./preload/index.js"
      },
      "import": {
        "types": "./esm/preload/index.d.ts",
        "default": "./esm/preload/index.js"
      }
    },
    "./utility": {
      "require": {
        "types": "./utility/index.d.ts",
        "default": "./utility/index.js"
      },
      "import": {
        "types": "./esm/utility/index.d.ts",
        "default": "./esm/utility/index.js"
      }
    },
    "./native": {
      "require": {
        "types": "./native/index.d.ts",
        "default": "./native/index.js"
      },
      "import": {
        "types": "./esm/native/index.d.ts",
        "default": "./esm/native/index.js"
      }
    }
  },
  "repository": "https://github.com/getsentry/sentry-electron.git",
  "author": "Sentry",
  "contributors": [
    "Tim Fish"
  ],
  "license": "MIT",
  "types": "./index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "set-version": "node scripts/update-version.js",
    "prebuild": "yarn clean && yarn set-version",
    "build": "rollup --config rollup.config.mjs",
    "postbuild": "node scripts/check-exports.mjs && node scripts/check-types.mjs && node scripts/check-loading.mjs && yarn pack",
    "clean": "rimraf --glob coverage common esm main preload renderer index.* sentry-electron*.tgz .eslintcache",
    "prelint": "yarn set-version",
    "lint": "run-s lint:oxfmt lint:eslint",
    "lint:oxfmt": "oxfmt --check \"{scripts,src,test,examples}/**/*.{ts,js}\"",
    "lint:eslint": "eslint . --cache --format stylish",
    "fix": "run-s fix:oxfmt fix:eslint",
    "fix:oxfmt": "oxfmt --write \"{scripts,src,test,examples}/**/*.{ts,js}\"",
    "fix:eslint": "eslint . --format stylish --fix",
    "update-electron-versions": "node ./scripts/update-electron-versions.mjs",
    "update-sdk-versions": "node ./scripts/update-sdk-versions.mjs",
    "pretest": "yarn set-version",
    "test": "vitest run --root=./test/unit",
    "pree2e": "yarn set-version && rimraf --glob test/e2e/dist/**/node_modules/@sentry/** test/e2e/dist/**/yarn.lock test/e2e/dist/**/package-lock.json && node scripts/clean-cache.js",
    "e2e": "xvfb-maybe vitest run --root=./test/e2e --silent=false --disable-console-intercept"
  },
  "dependencies": {
    "@sentry/browser": "10.40.0",
    "@sentry/core": "10.40.0",
    "@sentry/node": "10.40.0"
  },
  "peerDependencies": {
    "@sentry/node-native": "10.40.0"
  },
  "peerDependenciesMeta": {
    "@sentry/node-native": {
      "optional": true
    }
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^12.1.3",
    "@sentry-internal/eslint-config-sdk": "10.40.0",
    "@sentry-internal/typescript": "10.40.0",
    "@sentry/node-native": "10.40.0",
    "@types/busboy": "^1.5.4",
    "@types/koa": "^2.0.52",
    "@types/koa-bodyparser": "^4.3.0",
    "@types/tmp": "^0.2.2",
    "busboy": "^1.6.0",
    "electron": "^25.9.8",
    "eslint": "8.57.0",
    "eslint-plugin-require-extensions": "^0.1.3",
    "extract-zip": "^2.0.1",
    "koa": "^3.0.1",
    "koa-bodyparser": "^4.3.0",
    "koa-tree-router": "^0.12.1",
    "npm-run-all2": "^8.0.4",
    "oxfmt": "^0.32.0",
    "rimraf": "^6.0.1",
    "rollup": "^4.44.1",
    "tmp": "^0.2.1",
    "typescript": "~5.0.0",
    "vitest": "^4.0.18",
    "xvfb-maybe": "^0.2.1",
    "yaml": "^2.2.1"
  },
  "volta": {
    "node": "22.19.0",
    "yarn": "1.22.22"
  }
}